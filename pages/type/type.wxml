          <view class="nav">
            <view>
              <image src="{{item.img}}" style="width:38px;height:38px;"></image>
            </view>
            <view>{{item.name}}</view>
          </view>
        </block>
      </block>
    </view>
    <view class="navs">
      <block wx:for="{{iconSet3}}" wx:key="">
        <block wx:if="{{index >= 4}}">
          <view class="nav">
            <view>
              <image src="{{item.img}}" style="width:38px;height:38px;"></image>
            </view>
            <view>{{item.name}}</view>
          </view>
        </block>
      </block>
      <view class="nav"></view>
    </view>
  </view>
  <view class="line"></view>
</view>
